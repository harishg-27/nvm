const date = new Date("01/27/2022")
const d = date.getDay()
console.log("date",date,"d  ---",d)

if( d === 1 ){
    console.log("Monday")
}else if( d === 2) {
    console.log("Tuesday")
}else if( d === 3 ){
    console.log("wednesday")
}else if( d === 4 ){
    console.log("Thursday")
}else if( d === 5 ){
    console.log("Friday")
}else if( d === 6 ){
    console.log("Saturday")
}else{
    console.log("Sunday")
}

